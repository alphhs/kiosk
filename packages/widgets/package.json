{
  "name": "@kiosk/widgets",
  "version": "1.0.7",
  "private": true,
  "type": "module",
  "main": "./dist/widgets-render.js",
  "types": "./dist/widgets-render.d.ts",
  "exports": {
    ".": {
      "types": "./dist/widgets-render.d.ts",
      "import": "./dist/widgets-render.js"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "import": "./dist/registry.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./background-render": {
      "types": "./dist/background-render.d.ts",
      "import": "./dist/background-render.js"
    },
    "./blocks": {
      "types": "./dist/blocks/index.d.ts",
      "import": "./dist/blocks/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/widgets-render.tsx src/registry.tsx src/types.ts src/background-render.tsx src/blocks/index.tsx src/components/index.tsx --format esm --dts --clean",
    "dev": "tsup src/widgets-render.tsx src/registry.tsx src/types.ts src/background-render.tsx src/blocks/index.tsx src/components/index.tsx --format esm --dts --clean --watch",
    "check-types": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "preview": "serve storybook-static",
    "clean": "rm -rf dist .turbo && rm -rf node_modules",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:local": "graphql-codegen --config codegen.local.ts",
    "test": "vitest run"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "swiper": "^11.0.0"
  },
  "dependencies": {
    "@apollo/client": "3.13.5",
    "@cody-mn/utils": "^0.1.3",
    "@heroicons/react": "^2.2.0",
    "@kiosk/ui": "workspace:*",
    "@lingui/core": "^5.5.1",
    "@types/lodash": "^4.17.20",
    "classnames": "^2.3.2",
    "lodash": "^4.17.21",
    "rc-image": "^7.9.0"
  },
  "optionalDependencies": {
    "@react-google-maps/api": "^2.19.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^5.1.0",
    "@graphql-codegen/near-operation-file-preset": "3.0.0",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@graphql-codegen/typescript-react-apollo": "4.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "postcss": "^8.5.6",
    "sass": "^1.93.2",
    "serve": "^14.0.0",
    "storybook": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "~5.9.3",
    "vitest": "^2.0.0"
  }
}
