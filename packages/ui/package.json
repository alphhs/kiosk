{
    "name": "@kiosk/ui",
    "version": "0.2.1",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./countdown": {
            "types": "./dist/countdown.d.ts",
            "import": "./dist/countdown.js"
        },
        "./swiper": {
            "types": "./dist/swiper/index.d.ts",
            "import": "./dist/swiper/index.js"
        },
        "./image": {
            "types": "./dist/image.d.ts",
            "import": "./dist/image.js"
        },
        "./picture": {
            "types": "./dist/picture.d.ts",
            "import": "./dist/picture.js"
        },
        "./lazyload": {
            "types": "./dist/lazy-load/index.d.ts",
            "import": "./dist/lazy-load/index.js"
        },
        "./lottie": {
            "types": "./dist/lottie/index.d.ts",
            "import": "./dist/lottie/index.js"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format esm --dts --clean",
        "dev": "tsup src/index.ts --format esm --dts --clean --watch",
        "check-types": "tsc --noEmit",
        "lint": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "clean": "rm -rf dist .turbo"
    },
    "peerDependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "swiper": "^11.0.0",
        "lottie-web": "^5.12.2"
    },
    "dependencies": {
        "@cody-mn/utils": "^0.1.3",
        "classnames": "^2.3.2",
        "react-intersection-observer": "^9.0.0"
    },
    "devDependencies": {
        "@types/react": "^19.1.16",
        "@types/react-dom": "^19.1.9",
        "eslint": "^9.36.0",
        "tsup": "^8.0.0",
        "typescript": "~5.9.3"
    }
}
